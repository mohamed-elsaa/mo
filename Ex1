                for (int G = 1; G <= 1; G++) {
                    Mahmoud19();
                }
                for (int G = 1; G <= 1; G++) {
                    Mahmoud20();
                }
                for (int G = 1; G <= 1; G++) {
                    Mahmoud21();
                }
                System.out.println("                 *****************************************************");


                for (int A = 1; A <= 15; A++) {
                    System.out.println("*");
                }


            }


